<h1>Write a post here!</h1>
<div>
  <%= form_for(:post, url: posts_path) do |f| %>

    <%= f.label :body %>
    <%= f.text_area :body, class: "form-control" %>

    <%= f.submit "Create post!", class: "btn btn-primary" %>

  <% end %>
</div>
<br />
<div class="container">
  <%= debug(params) if Rails.env.development? %>
</div>
