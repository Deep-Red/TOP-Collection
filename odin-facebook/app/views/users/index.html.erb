<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table class="paddingBetweenCols">
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
      <td>
        <%= render user %>
      </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
